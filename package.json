{
  "name": "tictactoe",
  "version": "0.0.3",
  "private": true,
  "description": "TicTacToe Game",
  "dependencies": {},
  "scripts": {
    "build": "webpack --config ./app/webpack.config.js --mode production",
    "build-dev": "webpack --config ./app/webpack.config.js --mode development",
    "build-server": "webpack --config ./app/server/webpack.config.js --mode production",
    "build-server-dev": "webpack --config ./app/server/webpack.config.js --mode development",
    "build-web": "webpack --config ./app/web/webpack.config.js --mode production",
    "build-web-dev": "webpack --config ./app/web/webpack.config.js --mode development",
    "build-mobile": "webpack --config ./app/mobile/TicTacToe/webpack.config.js --mode production",
    "build-mobile-dev": "webpack --config ./app/mobile/TicTacToe/webpack.config.js --mode development",
    "deploy": "gulp",
    "test": "mocha --require babel-register ./test/**/test_*.js",
    "start-server": "node ./deploy/server/main.js"
  },
  "author": "evg656e <evg656e@gmail.com>",
